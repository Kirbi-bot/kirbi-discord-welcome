{
	"name": "kirbi-discord-welcome",
	"description": "adds a command to Kirbi for displaying contents of a file, aimed specifically at a welcome channel",
	"version": "0.1.0",
	"license": "Apache-2.0",
	"readme": "README.md",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Richardson-Media-House/kirbi-discord-welcome.git"
	},
	"author": "Matt Carroll <admin@develogix.com> (https://develogix.com)",
	"contributors": [
		"Nate Richardson <hello@naterichardson.com> (https://naterichardson.com)",
		"Joseph Moran III <joseph.moraniii@gmail.com> (https://josephmoran.co)"
	],
	"main": "welcome.js",
	"dependencies": {
		"chalk": "^2.3.0",
		"mongoose": "^4.12.4",
		"kirbi-discord": "github:Richardson-Media-House/kirbi-discord",
		"kirbi-mongodb": "github:Richardson-Media-House/kirbi-mongodb"
	},
	"devDependencies": {
		"xo": "^0.18.2"
	},
	"xo": {
		"rules": {
			"linebreak-style": "off",
			"no-prototype-builtins": "warn",
			"radix": "warn",
			"guard-for-in": "warn",
			"import/no-unresolved": "off",
			"camelcase": "off",
			"no-negated-condition": "warn",
			"object-curly-spacing": [
				"error",
				"always",
				{
					"arraysInObjects": false
				}
			]
		}
	},
	"scripts": {
		"test": "xo"
	}
}
