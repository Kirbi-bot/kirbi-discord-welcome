{
    "name": "kirbi-welcome",
    "description": "adds a command to Kirbi for displaying contents of a file, aimed specifically at a welcome channel",
    "version": "0.1.0",
    "license": "Apache-2.0",
    "readme": "README.md",
    "repository": {
      "type": "git",
      "url": "git+https://github.com/Richardson-Media-House/kirbi-welcome.git"
    },
    "author": "Nate Richardson <hello@naterichardson.com> (https://naterichardson.com)",
    "contributors": [
      "Matt Carroll <admin@develogix.com> (https://develogix.com)",
      "Joseph Moran III <joseph.moraniii@gmail.com> (https://josephmoran.co)"
    ],
    "main": "welcome.js",
    "dependencies": {},
    "devDependencies": {
      "xo": "^0.18.2"
    },
    "xo": {
      "rules": {
        "linebreak-style": "off",
        "no-prototype-builtins": "warn",
        "radix": "warn",
        "guard-for-in": "warn", 
        "import/no-unresolved": "off",
        "camelcase": "off",
        "no-negated-condition": "warn",
        "object-curly-spacing": [
          "error",
          "always",
          {
            "arraysInObjects": false
          }
        ]
      }
    },
    "scripts": {
      "test": "xo"
    }
  }
  